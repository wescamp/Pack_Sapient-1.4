#: Pack_Sapient/scenarios/2p_Tomb_of_Kings.cfg:53
#: Pack_Sapient/scenarios/4p_Mega_2v2.cfg:99
#: Pack_Sapient/scenarios/4p_Mega_2v2.cfg:121
msgid "End of turns."
msgstr "Vuorojen loppu."

#. [objectives]
#: Pack_Sapient/scenarios/2p_Tomb_of_Kings.cfg:56
msgid "A section of the tomb wall crumbles on turn 20."
msgstr "Osa haudasta romahtaa vuorolla 20."

#. [message]: speaker=narrator
#: Pack_Sapient/scenarios/2p_Tomb_of_Kings.cfg:71
msgid "The earth shakes."
msgstr "Maa tärisee."

#. [multiplayer]: id=multiplayer_Mega_2v2
